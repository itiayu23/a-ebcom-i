<h1>Admin::Contacts#index</h1>
<table class="table table-hover table-inverse">
<thead>
  <tr>
    <th>名前</th>
    <th>ニックネーム</th>
    <th>問い合わせ本文</th>
  </tr>

</thead>


<% @contacts.each do |contact| %>
<tbody>
  <tr>
    <td>
        <%= link_to admin_contacts_show_path(contact.id) do %>
        <%= contact.name %>
        </td>
    <td><%= contact.nickname %></td>
    <td><%= contact.message %></td>
    <% end %>
  </tr>
</tbody>
</table>
<% end %>
