<h1>お問い合わせ一覧</h1>
<table class="table table-hover table-inverse">
<thead>
  <tr>
    <th>名前</th>
    <th>ニックネーム</th>
    <th>問い合わせ本文</th>
  </tr>

</thead>



<tbody>
  <% @contacts.each do |contact| %>
  <tr>
    <td>
        <%= contact.name %>
        </td>
    <td><%= contact.nickname %></td>

    <td><%= link_to admin_contact_path(contact.id) do %>
        <%= contact.message %>
    <% end %></td>
  </tr>
  <% end %>
</tbody>
</table>

